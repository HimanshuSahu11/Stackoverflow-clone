@model List<StackOverflowProject.ViewModels.QuestionViewModel>
@{ 
    ViewBag.Title = "Home";
}

    <div class="row">
        <div class="col-sm-12">
            <div class="container-fluid text-sm-center p-5 " style="background-color:#2692b3">
                <h1>Learn,Shared,Build</h1>
                <h3>Each month over 50 million developer come to stackoverflow to learn,share their knowledge andd build thier careers</h3>
                <p class="lead">Join the world's largest developer community</p>
            </div>
        </div>
    </div>

<div class="row">
    <div class="col-sm-12">
        <table class="table">
            @foreach (var item in Model)
            {
                Html.RenderPartial("Question", item);
            }
        </table>
        @if (Session["CurrentUserName"]!=null)
        {
            <a href="/questions/create" class="btn btn-primary">Add Question</a>
        }
    </div>
</div>

<script>
    $("a[href='/home/index']").attr("id", "active");
</script>